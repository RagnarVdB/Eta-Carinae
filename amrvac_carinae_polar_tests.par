! setup.pl -d=2
 &filelist
	base_filename='output0/wind_2d_eta_'
        autoconvert=.true.
        saveprim=.true.
        nwauxio=1
	convert_type='vtuCCmpi'
       !nocartesian=.true.
 /
!	

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        ditsave_log=1
        dtsave_dat=1.25d-4 !save each year is then secondsinyear/computerunit time
 /

 &stoplist
        it_max=10000000
        time_max=2.6d-1 !This achieved by dividing 707*secondinyear/computerunit
 /

 &methodlist
        time_stepper='threestep'
        flux_scheme=20*'hll'
        limiter=20*'koren'
 /
!        flux_scheme=20*'hllc'

 &boundlist
        typeboundary_min1=4*'cont'
        typeboundary_max1=4*'cont'
        typeboundary_min2=4*'cont'
        typeboundary_max2=4*'cont'
 / 
!        typeboundaryexample_min1='symm','asymm','symm','symm'

 &meshlist
        refine_criterion=3
        refine_max_level=4
        w_refine_weight(1)=0.5
        w_refine_weight(4)=0.5
        block_nx1=8
        block_nx2=8
        domain_nx1=64
        domain_nx2=64
        xprobmin1=0.0d0
        xprobmax1=4.0d1
        xprobmin2=0.d0
        xprobmax2=1.0d0
 /

 &paramlist
	!dtpar = 1.0d-3
        typecourant='maxsum'
        courantpar=0.1d0
	slowsteps=1000
 /
&hd_list
	hd_radiative_cooling=.true.
/
&rc_list
	coolcurve='MLcosmol'
	coolmethod='exact'
!Not physical but recommended
	Tfix=.true.
!Set lower temperature to 10^2 K
	tlow = 1.0d0 !Since 10^2/unit_temp = 1
/
